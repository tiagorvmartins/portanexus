    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "i18n-js": "^4.5.1",
    "inversify": "^7.3.0",
    "mobx": "^6.13.7",
    "mobx-react": "^9.2.0",
    "moment": "^2.30.1",
    "patch-package": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
