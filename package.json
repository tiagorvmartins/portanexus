    "@react-navigation/drawer": "^7.3.2",
    "@react-navigation/native": "^7.0.14",
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.8.4",
    "class-transformer": "^0.5.1",
    "expo": "^53.0.22",
    "expo-blur": "^14.1.5",
    "expo-build-properties": "^0.14.8",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linking": "~7.1.7",
    "expo-localization": "^16.1.6",
    "expo-router": "~5.1.5",
