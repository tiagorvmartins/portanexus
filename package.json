{
  "name": "portanexus",
  "description": "A mobile connection for your Portainer instance.",
  "author": "tiagorvmartins",
  "license": "MIT",
  "main": "./index.js",
  "version": "0.2.2",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android --tunnel --clear",
    "ios": "expo start --ios",
    "web": "expo start --web -c",
    "web-production": "expo export -p web && npx workbox-cli generateSW workbox-config.js",
    "build-android": "eas build --platform android --local --non-interactive --profile preview --output portanexus_0.2.2.apk",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-picker/picker": "^2.11.1",
